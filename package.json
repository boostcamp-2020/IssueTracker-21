	},
	"homepage": "https://github.com/Changyu-Ryou/javascript-w5-accountbook#readme",
	"dependencies": {
		"clean-webpack-plugin": "^3.0.0",
		"concurrently": "^5.3.0",
		"cookie-parser": "~1.4.4",
		"core-js": "^3.6.5",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"html-loader": "^1.3.1",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"mysql2": "^2.2.5",
